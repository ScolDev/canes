{
  "name": "canes",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "eslint": "eslint . --ext .ts,.tsx",
    "ts-check": "tsc --noemit",
    "lint": "npm run eslint && npm run ts-check",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "benchmark:basic": "npm run benchmark",
    "benchmark:average": "npm run benchmark -- --average",
    "benchmark": "npx ts-node --esm --experimental-specifier-resolution=node  tests/benchmark/handler.ts",
    "prepare": "husky install",
    "pre-commit": "npm run lint && git add . && npm t",
    "preview": "vite preview"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "testMatch": [
      "**/?(*.test).ts?(x)"
    ],
    "setupFiles": [
      "<rootDir>/tests/config.ts"
    ],
    "testEnvironment": "jsdom"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/plugin-transform-modules-commonjs": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/jest": "^26.0.24",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.0",
    "babel-jest": "^27.0.6",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.0",
    "jest": "^27.0.6",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  }
}
